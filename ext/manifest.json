{
  "manifest_version": 2, 

  "name": "Plants vs Humans",
  "description": "This Extension will grow a plant over your currently used website",
  "version": "1.0",
  "permissions": [
    "activeTab",
    "tabs"
  ],

  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },

  "browser_action": {
    "default_title": "Grow strong!"
  }
}
