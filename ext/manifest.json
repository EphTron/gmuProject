  //     "js": ["myscript.js"],
  //     "run_at": "document_end"
  //   }
  // ],

  // "background": {
  //   "scripts": ["bg.js"],
  //   "persistent": false
  // },

  "browser_action": {
    "default_title": "Grow strong!"
  }


}